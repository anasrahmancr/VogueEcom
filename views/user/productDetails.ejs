    <script>
        
        function addtocart(prodId){
            let quantity = document.getElementById('qty').innerHTML
            fetch("/addtoCart/"+ prodId, {
    method: "POST",
    headers: {
      "Content-Type": "application/json"
    },
    body: JSON.stringify({ quantity: quantity })
  })
  .then(function(response) {

    if (!response.ok) {
      throw new Error("Network response was not ok");
      console.log('toastr errror ');
    }
    // Handle successful response from server
    // toastr.success('Product added to cart')
    Toastify({
  text: "PRODUCT ADDED TO THE CART",
  duration: 3000,
//   destination: "https://github.com/apvarun/toastify-js",
  newWindow: true,
  close: true,
  gravity: "top", // `top` or `bottom`
  position: "center", // `left`, `center` or `right`
  stopOnFocus: true, // Prevents dismissing of toast on hover
  style: {
    background: "linear-gradient(to right, #00b09b, #96c93d)",
  },
  onClick: function(){} // Callback after click
}).showToast();
    
  })
  .catch(function(error) {
    console.error("Error:", error);
  });
};
  


    </script>
    
    <script>
    
        function addtowishlist(event, productId) {
            console.log('test');
      event.preventDefault(); // Prevent the default behavior of the link
      
      fetch(`/addToWishlist/${productId}`, {
        method: 'POST', // You can change the HTTP method as needed
        headers: {
          'Content-Type': 'application/json'
        },
        // You can include additional request parameters or data here
      })
        .then(response => response.json())
        .then(data => {
            console.log(data,"dddd")
            Toastify({
                text: data.message,
                duration: 1000,
    
                newWindow: true,
                close: true,
                gravity: "top", // `top` or `bottom`
                position: "center", // `left`, `center` or `right`
                stopOnFocus: true, // Prevents dismissing of toast on hover
                style: {
                  background: "linear-gradient(to right, #00b09b, #96c93d)",
                },
                onClick: function () {}, // Callback after click
              }).showToast();
          // Handle the response from the server
          console.log(data);
        })
        .catch(error => {
          console.log(error);
        });
    }
    </script>